
<div class="clearfix">
  <a class="btn btn-primary" href="/admin/storys/new" style="margin-left: 10px;">新建帖子</a>
</div>

<hr>

<div>
  <table class="table table-borderless table-sortable">
    <thead>
    <tr>
      <th data-sort="int">#</th>
      <th data-sort="string">标题</th>
      <th data-sort="string">类型</th>
      <th data-sort="int" data-sort-default="desc" data-sort-onload>创建时间</th>
    </tr>
    </thead>
    <tbody>
    <% storys.forEach (function (storys){ %>
    <tr>
      <td class="table-id" data-sort-value="<%=story.objectId%>">#<%= story.objectId%></td>
      <td class="table-title"><a href="/story/<%= story.objectId %>" title="#<%= story.objectId
        %>"><%=
            story.title %></a></td>
      <td class="table-type"><%= story.type %></td>
      <td class="table-date" data-sort-value="<%= story.createdAtUnix %>"><span class="bstooltip" title="<%= story.createdAtLong %>"><%= story.createdAt %></span></td>
    </tr>
    <% }) %>
    </tbody>
  </table>
  <p>
    提示：
  </p>
</div>
